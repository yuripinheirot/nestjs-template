{
  "name": "nestjs-template",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch --debug 0.0.0.0:9229",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "yarn node --expose-gc --no-compilation-cache ./node_modules/.bin/jest --detectOpenHandles --forceExit --runInBand --logHeapUsage --passWithNoTests --noStackTrace",
    "test:v": "yarn node --expose-gc --no-compilation-cache ./node_modules/.bin/jest --detectOpenHandles --forceExit --logHeapUsage",
    "test:w": "yarn test --verbose --watch",
    "test:c": "yarn test --coverage",
    "test:u": "yarn test -c jest-unit-config.js",
    "test:f": "yarn test:v -c jest-feature-config.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.10",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.10",
    "@nestjs/platform-express": "^11.0.10",
    "@nestjs/swagger": "^11.0.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.4",
    "@nestjs/testing": "^11.0.10",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "jest": "^29.7.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "typescript": "^5.7.3"
  }
}
